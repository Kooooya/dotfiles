	insteadOf = https://github.com/

[user]
	email = fukushi098@gmail.com\n
	name = Koya Fukushi\n
[user]
        name = koya
        email = fukushi098@gmail.com
[alias]
        co = checkout
        cm = commit
        gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
        s = status
	b = branch
	a = add
	df = diff
	cmm = commit -m
	rs = reset
	ad = add
	l = log
[core]
        editor = vim
        excludesfile = ~/.gitignore_global
[color]
        diff = auto
    status = auto
    branch = auto
[gc]
	auto = 0
